{
  "name": "backend", 
  "version": "1.0.1",
  "description": "Backend para sistema E-Commers con Node.js, Express y Sequelize",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node config/createDataBase.js",
    "reset-db" : "node config/resetDataBase.js",
    "seed" : "node -e \"require('./seeders/adminSeeder').runSeeders().then() => process.exit(0)).catch((err) => { console.error(err):process .exit(1); }}\" ", 
    "test" : "jest --detectOpenHandles --forceExit",
    "text:watch" : "jest --watch",
    "test:converge" : "jest --coverage"
  },
  "lockfileVersion": 3,
  "requires": true,
  "packages": {},

  "keywords" : [
  "e-commerce",
  "nodejs",
  "express",
  "sequelize",
  "jwt",
  "mysql"
  ],

  "author" : "Moonless'Night",
  "licence" : "ISC",
  "depedencias" : {
    "bcrypt" : "^2.4.3",
    "cors" : "^2.8.5",
    "dotenv" : "^16.3.1",
    "express" : "^4.18.2",
    "express-validation" : "^70.1",
    "jsonwebtoke" : "^9.0.2",
    "multer" : "^1.4.5-lts.1",
    "sequelize" : "^6.35.2",
    "mysql2" : "^3.6.5"
  },

  "devDependencias" : {
    "@types/jest" : "^30.0.0",
    "jest" : "^30.2.0",
    "modemon" : "^3.0.2",
    "supertest" : "^7.2.2"
  }
}

